<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/friends_list_header"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/imageView8" />





    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/friends_list_item_background"
        android:layout_below="@+id/imageView8"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/imageView9" />

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="430dp"
        android:background="@drawable/friend_detail_centre_background"
        android:id="@+id/imageView7"
        android:layout_below="@+id/imageView9"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageView7"
        android:textColor="#00ff00"
        android:textSize="20dp"
        android:layout_marginLeft="40dp"
        android:layout_marginStart="50dp"
        android:layout_marginBottom="11dp"
        android:id="@+id/textView_name1"
         />



    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/list_footer"
        android:id="@+id/imageView10"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:src="@drawable/button_enemies"
        android:layout_width="130dp"
        android:layout_height="40dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="5dp"
        android:layout_marginEnd="5dp"
        android:id="@+id/imageButton19"
        android:layout_alignTop="@+id/imageView10" />

    <ImageButton
        android:src="@drawable/button_friends_list"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:id="@+id/button_list" />

    <ImageButton
        android:src="@drawable/button_friends_list_edit"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/imageButton19"
        android:layout_alignEnd="@+id/imageButton19"
        android:layout_marginRight="12dp"
        android:layout_marginEnd="12dp"
        android:id="@+id/button_edit" />

    <ImageButton
        android:src="@drawable/button_list_delete"
        android:layout_width="60dp"
        android:layout_height="30dp"
        android:layout_below="@+id/imageView8"
        android:layout_alignRight="@+id/button_edit"
        android:layout_alignEnd="@+id/button_edit"
        android:id="@+id/button_delete" />

    <ImageButton
        android:src="@drawable/button_radar"
        android:layout_width="130dp"
        android:layout_height="40dp"
        android:id="@+id/Button_radar"
        android:layout_alignTop="@+id/imageView10"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button_delete"
        android:layout_toRightOf="@+id/Button_radar"
        android:layout_toEndOf="@+id/Button_radar"
        android:layout_marginTop="30dp"
        android:id="@+id/textView_num1" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView_num1"
        android:layout_toRightOf="@+id/textView_num1"
        android:layout_toEndOf="@+id/textView_num1"
        android:layout_marginLeft="14dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="65dp"
        android:id="@+id/textView_jw" />

    <TextView
        android:text="21.3M"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView_jw"
        android:layout_alignLeft="@+id/textView_num1"
        android:layout_alignStart="@+id/textView_num1"
        android:layout_marginTop="33dp"
        android:id="@+id/textView_at" />

    <TextView
        android:text="5M"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView_at"
        android:layout_toRightOf="@+id/textView_name1"
        android:layout_toEndOf="@+id/textView_name1"
        android:layout_marginLeft="15dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="34dp"
        android:id="@+id/textView_ac" />

    <TextView
        android:text="GUANGDONG CHINA"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView_ac"
        android:layout_alignLeft="@+id/textView_jw"
        android:layout_alignStart="@+id/textView_jw"
        android:layout_marginTop="38dp"
        android:id="@+id/textView_ad" />

    <TextView
        android:text="1''223"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView_ad"
        android:layout_alignLeft="@+id/button_delete"
        android:layout_alignStart="@+id/button_delete"
        android:layout_marginTop="52dp"
        android:id="@+id/textView_last" />

    <TextView
        android:text="6''312"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView7"
        android:layout_alignRight="@+id/textView_last"
        android:layout_alignEnd="@+id/textView_last"
        android:layout_marginRight="15dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="23dp"
        android:id="@+id/textView_next" />




</RelativeLayout>