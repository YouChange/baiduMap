<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#ffffff"
    android:descendantFocusability="blocksDescendants"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="60dp"
        android:focusable="false"
        android:clickable="false"
        android:focusableInTouchMode="false"
        android:background="@drawable/friends_list_item_background"
        android:layout_alignParentTop="true"
        android:id="@+id/imageView6" />



    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:focusable="false"
        android:clickable="false"
        android:focusableInTouchMode="false"
        android:layout_alignBottom="@+id/imageView6"
        android:textSize="20dp"
        android:textColor="#00FF00"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="77dp"
        android:layout_marginStart="77dp"
        android:layout_marginBottom="18dp"
        android:id="@+id/textView_name" />

    <TextView
        android:text="TextView"
        android:visibility="invisible"
        android:focusable="false"
        android:clickable="false"
        android:focusableInTouchMode="false"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView_name"

        android:layout_marginRight="36dp"
        android:layout_marginEnd="36dp"
        android:id="@+id/textView_id" />



    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:textColor="#00FF00"
        android:layout_alignBottom="@+id/textView_name"
        android:layout_toRightOf="@+id/textView_name"
        android:layout_toEndOf="@+id/textView_name"
        android:layout_marginLeft="17dp"
        android:layout_marginStart="17dp"
        android:id="@+id/textView_num" />
</RelativeLayout>